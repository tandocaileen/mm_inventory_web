{
    "buildCommand": "flutter build web --release --web-renderer canvaskit",
    "outputDirectory": "build/web",
    "installCommand": "if cd flutter; then git pull && cd ..; else git clone https://github.com/flutter/flutter.git; fi && flutter/bin/flutter doctor && flutter/bin/flutter pub get",
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/manifest.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                },
                {
                    "key": "Content-Type",
                    "value": "application/manifest+json"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}